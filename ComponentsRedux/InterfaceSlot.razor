@inherits BaseClass

<div class="tb-integrationslot @CssClass"
     ondragover="event.preventDefault()"
     @ondragenter="HandleOnDragEnter"
     @ondragleave="HandleOnDragLeave"
     @ondrop:stopPropagation
     @ondrop="HandleOnDrop">

    @if ((Parent as IntegrationNode).Interfaces[Position] != null) {
        <Interface Guid=@((Parent as IntegrationNode).Interfaces[Position].Guid) Title=@((Parent as IntegrationNode).Interfaces[Position].Title) Parent=@(this) @key=@((Parent as IntegrationNode).Interfaces[Position].Guid)></Interface>
    }

</div>